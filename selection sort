arr=[11,15,12,14,18,13]
for i in range(len(arr)-1):
    min=i
    for j in range(i+1,len(arr)):
        if arr[i]>arr[j]:
            min=j
            arr[i],arr[min]=arr[min],arr[i]
            print(arr)
            print(arr)



arr=[11,15,12,14,18,13]
for i in range(len(arr)-1):
    min=i
    for j in range(i+1,len(arr)):
        if arr[min]>arr[j]:
            min=j
    print([min,i],end=",")
    print()
    arr[i],arr[min]=arr[min],arr[i]
    #print(arr)
print(arr)


